   "source": [
    "target = 999\n",
    "def sum_divisible_by(n):\n",
    "    p = target // n\n",
    "    return n * (p * (p + 1)) // 2\n",
    "\n",
    "sum_divisible_by(3) + sum_divisible_by(5) - sum_divisible_by(15)"
   ]
